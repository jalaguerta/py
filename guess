import random
winFlag = 0
number = random.randint(1,101)
guesses = [0]

while winFlag == 0:

    guess = int(input("Enter a number between 1 and 100: "))
    guesses.append(guess)
    if (guess == number):
        print("You win!")
        winFlag = 1
        break
    elif (guess < 1 and guess > 100):
        print ("Out of bounds")
    elif (abs(guess-number) < 10 and len(guesses) < 2):
        print ("Warm!")
    else:
        print("Cold...")
    guesses
    
    pass
